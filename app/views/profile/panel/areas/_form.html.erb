<%= form_with model: [:admin, @area] do |f| %>
  <%= render 'shared/error_messages', target: @area %>
  <%= render partial: 'nodes/siblings_select', locals: { f: f, node: Area.new } %>
  <%= f.text_field :name %>
  <%= f.check_box :popular %>
  <%= f.check_box :published %>
  <%= f.submit %>
<% end %>
