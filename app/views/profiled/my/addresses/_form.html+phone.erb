<%= render 'error_messages', target: f.object %>
<%= f.text_field :contact %>
<%= f.text_field :tel %>
<div class="weui-cell weui-cell_active">
  <div class="weui-cell__hd">
    <label class="weui-label"><%= Profiled::Address.human_attribute_name(:area_id) %></label>
  </div>
  <div class="weui-cell__bd" data-controller="weui-picker" data-weui-picker-id-value="address_area_id" data-weui-picker-url-value="<%= url_for(controller: '/profiled/areas', area_id: f.object.area_id) %>">
    <%= f.hidden_field :area_id %>
    <input class="weui-input" type="text" data-action="click->weui-picker#getData">
  </div>
</div>
<%= f.text_area :detail %>
